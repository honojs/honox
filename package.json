{
  "name": "honox",
  "version": "0.1.54",
  "type": "module",
  "scripts": {
    "prepare": "tsdown",
    "test": "bun typecheck && bun test:unit && bun test:integration && bun test:e2e",
    "test:unit": "vitest --run ./src",
    "test:integration": "bun test:integration:api && bun test:integration:apps",
    "test:integration:api": "vitest --run ./test-integration/api.test.ts",
    "test:integration:apps": "vitest --run -c ./test-integration/vitest.config.ts ./test-integration/apps.test.ts",
    "test:e2e": "playwright test -c ./test-e2e/playwright.config.ts ./test-e2e/e2e.test.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsdown && publint",
    "watch": "tsdown --watch",
    "lint": "eslint src mocks test-integration test-e2e",
    "lint:fix": "eslint src mocks test-integration test-e2e --fix",
    "format": "prettier --check \"src/**/*.{js,ts}\" \"mocks/**/*.{js,ts}\" \"test-*/**/*.{js,ts}\"",
    "format:fix": "prettier --write \"src/**/*.{js,ts}\" \"mocks/**/*.{js,ts}\" \"test-*/**/*.{js,ts}\"",
    "prerelease": "bun run test && bun run build",
    "release": "np"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/client/index.js",
    "./client/client": "./dist/client/client.js",
    "./client/runtime": "./dist/client/runtime.js",
    "./client/utils/filter": "./dist/client/utils/filter.js",
    "./constants": "./dist/constants.js",
    "./factory": "./dist/factory/index.js",
    "./factory/factory": "./dist/factory/factory.js",
    "./server": "./dist/server/index.js",
    "./server/base": "./dist/server/base.js",
    "./server/components": "./dist/server/components/index.js",
    "./server/components/has-islands": "./dist/server/components/has-islands.js",
    "./server/components/link": "./dist/server/components/link.js",
    "./server/components/script": "./dist/server/components/script.js",
    "./server/context-storage": "./dist/server/context-storage.js",
    "./server/server": "./dist/server/server.js",
    "./server/utils/file": "./dist/server/utils/file.js",
    "./server/utils/path": "./dist/server/utils/path.js",
    "./server/with-defaults": "./dist/server/with-defaults.js",
    "./types": "./dist/types.js",
    "./vite": "./dist/vite/index.js",
    "./vite/client": "./dist/vite/client.js",
    "./vite/components": "./dist/vite/components/index.js",
    "./vite/components/honox-island": "./dist/vite/components/honox-island.js",
    "./vite/inject-importing-islands": "./dist/vite/inject-importing-islands.js",
    "./vite/island-components": "./dist/vite/island-components.js",
    "./vite/restart-on-add-unlink": "./dist/vite/restart-on-add-unlink.js",
    "./vite/utils/path": "./dist/vite/utils/path.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types"
      ],
      "factory": [
        "./dist/factory"
      ],
      "server": [
        "./dist/server"
      ],
      "server/base": [
        "./dist/server/base"
      ],
      "client": [
        "./dist/client"
      ],
      "vite": [
        "./dist/vite"
      ],
      "vite/client": [
        "./dist/vite/client"
      ],
      "vite/components": [
        "./dist/vite/components"
      ]
    }
  },
  "author": "Yusuke Wada <yusuke@kamawada.com> (https://github.com/yusukebe)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honojs/honox.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "homepage": "https://hono.dev",
  "dependencies": {
    "@babel/generator": "7.25.6",
    "@babel/parser": "7.25.6",
    "@babel/traverse": "7.25.6",
    "@babel/types": "7.25.6",
    "@hono/vite-dev-server": "^0.23.0",
    "jsonc-parser": "3.3.1",
    "precinct": "12.2.0"
  },
  "overrides": {
    "@typescript-eslint/typescript-estree": "^8.19.0"
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "^8.19.0"
  },
  "peerDependencies": {
    "hono": ">=4.*"
  },
  "devDependencies": {
    "@hono/eslint-config": "^1.1.1",
    "@mdx-js/rollup": "^3.0.0",
    "@playwright/test": "^1.42.0",
    "@types/node": "^20.10.5",
    "eslint": "^9.23.0",
    "glob": "^10.3.10",
    "happy-dom": "^20.0.10",
    "hono": "^4.10.3",
    "np": "^10.2.0",
    "prettier": "^3.1.1",
    "publint": "^0.2.7",
    "tsdown": "^0.20.3",
    "typescript": "^5.3.3",
    "vite": "^5.4.12",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.3"
  },
  "packageManager": "bun@1.2.20",
  "engines": {
    "node": ">=18.14.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  }
}
